{
  "name": "react-calendar-price",
  "version": "0.1.0",
  "description": "React calendar with price or some message",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -c --port=8888",
    "build-js": "babel src --out-dir lib && webpack -p",
    "build-css": "sass src/scss/input-moment.scss dist/input-moment.css",
    "build": "npm run build-js && npm run build-css",
    "deploy": "npm run build && github-pages-deploy",
    "test": "jest",
    "clean": "rm dist/* && rm example/bundle*"
  },
  "github-pages-deploy": {
    "src": "example"
  },
  "author": "zhZhang",
  "license": "ISC",
  "peerDependencies": {
    "moment": "^2.10.6",
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "css-loader": "^0.21.0",
    "github-pages-deploy": "0.0.3",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "moment": "^2.17.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "i": "^0.3.5",
    "lodash": "^4.17.4",
    "npm": "^4.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dcatfly/react-calendar-price.git"
  },
  "keywords": [
    "react",
    "react-component",
    "input",
    "datetime",
    "picker",
    "price"
  ],
  "bugs": {
    "url": "https://github.com/Dcatfly/react-calendar-price/issues"
  },
  "homepage": "https://github.com/Dcatfly/react-calendar-price#readme"
}
